[alias]
	aa = add --all
	st = status -s
	dc = diff --cached
	ci = commit
	ca = commit --amend
	br = branch
	co = checkout
	di = diff
	ri = rebase -i
	pullff = pull --ff-only
[user]
	name = beerchili
	email = eugen.liebreich@gmail.com
[credential]
	helper = osxkeychain
[core]
	excludesFile = ~/.gitignore_global
	editor = vim
[init]
	defaultBranch = main
[status]
	showUntrackedFiles = all
